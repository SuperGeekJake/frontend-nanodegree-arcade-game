{
  "name": "frontend-nanodegree-arcade-game",
  "version": "0.1.0",
  "description": "FEND Frogger Clone",
  "main": "src/game.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "browserify src/game.js -o assets/js/build.js",
    "watch": "watchify src/game.js -v -o assets/js/build.js",
    "serve": "browser-sync start --server --port 3000 & watchify src/game.js -v -o assets/js/build.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SuperGeekJake/frontend-nanodegree-arcade-game.git"
  },
  "author": "Jacob Williams",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SuperGeekJake/frontend-nanodegree-arcade-game/issues"
  },
  "homepage": "https://github.com/SuperGeekJake/frontend-nanodegree-arcade-game#readme",
  "devDependencies": {
    "browser-sync": "^2.9.3",
    "browserify": "^11.0.1",
    "browserify-shim": "^3.8.10",
    "watchify": "^3.4.0"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "createjs": "global:createjs"
  }
}
